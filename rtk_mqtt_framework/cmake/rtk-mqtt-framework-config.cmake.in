@PACKAGE_INIT@

# RTK MQTT Framework CMake 配置檔案
# 用於其他專案尋找和連結此 framework

include(CMakeFindDependencyMacro)

# 尋找依賴套件
find_dependency(PkgConfig REQUIRED)
pkg_check_modules(PAHO_MQTT_C REQUIRED libpaho-mqtt3c)

# 設定變數
set(RTK_MQTT_FRAMEWORK_VERSION @PROJECT_VERSION@)
set(RTK_MQTT_FRAMEWORK_INCLUDE_DIRS "@CMAKE_INSTALL_FULL_INCLUDEDIR@/rtk_mqtt_framework")
set(RTK_MQTT_FRAMEWORK_LIBRARIES "@CMAKE_INSTALL_FULL_LIBDIR@/librtk_mqtt_framework.a")

# 導入目標
if(NOT TARGET rtk_mqtt_framework::rtk_mqtt_framework)
    include("${CMAKE_CURRENT_LIST_DIR}/rtk-mqtt-framework-targets.cmake")
endif()

# 檢查所有必要元件
check_required_components(rtk_mqtt_framework)